disabled rule("Legend Info in Spawn room")
{
	event
	{
		Ongoing - Each Player;
		All;
		All;
	}

	conditions
	{
		Is Button Held(Event Player, Button(Interact)) == False;
		Is In Spawn Room(Event Player) == False;
	}

	actions
	{
		Destroy All HUD Text;
		Wait(0.010, Ignore Condition);
		Call Subroutine(HUD);
	}
}

